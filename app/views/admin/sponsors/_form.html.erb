<%= simple_form_for([:admin, @sponsor], html: { multipart: true }) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.file_field :logo, label: 'Logo' %>
    <br>
  </div>

  <div class="form-actions">
    <%= f.button :submit, 'Save', class: 'btn btn-primary pull-right' %>
  </div>
<% end %>

<br>
